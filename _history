puesta():\n    global puntos\n    # Si la opción seleccionada coincide con la esperada\n    if opciones_text[seleccion] == expected:\n        puntos += 2\n        music.ba_ding.play()\n        # Mostrar la bandera del país si es correcta\n        scene.set_background_image(banderas[indice])\n    else:\n        music.wawawawaa.play()\n        # Si es incorrecta dejamos una señal (color rojo), o puedes elegir otra acción\n        scene.set_background_color(2)\n    pause(800)\n    mostrarpregunta()\n\n# ============================\n# Crear una nueva pregunta\n# ============================\ndef mostrarpregunta():\n    global indice, preguntaActual, expected, seleccion, opciones_text\n    # Elegir país al azar\n    indice = randint(0, len(paises) - 1)\n    preguntaActual = paises[indice]\n    expected = capitales[indice]\n    seleccion = 0\n    # Cambiar fondo por bandera (inicialmente la bandera del país)\n    scene.set_background_image(banderas[indice])\n    # Crear lista con las opciones (SOLO STRINGS)\n    opciones_text = []\n    opciones_text.append(expected)\n    # Agregar incorrectas únicas\n    while len(opciones_text) < 4:\n        idx = randint(0, len(capitales) - 1)\n        if capitales[idx] not in opciones_text:\n            opciones_text.append(capitales[idx])\n    # Mezclar manualmente\n    for i in range(4):\n        j = randint(0, 3)\n        temp = opciones_text[i]\n        opciones_text[i] = opciones_text[j]\n        opciones_text[j] = temp\n    mostrarOpciones()\n\n# ============================\n# Variables globales iniciales\n# ============================\nindice = 0\npuntos = 0\nexpected = \"\"\npreguntaActual = \"\"\ntexto = \"\"\nseleccion = 0\nopciones_text: = [\"\"]\nbanderas: List[Image] = []\ncapitales: = [\"\"]\npaises: = [\"\"]\n\npaises = [\"Colombia\", \"Francia\", \"Alemania\", \"Japon\"]\ncapitales = [\"Bogota\", \"Paris\", \"Berlin\", \"Tokio\"]\nbanderas = [\n    assets.image(\"\"\"\n        Col\n        \"\"\"),\n    assets.image(