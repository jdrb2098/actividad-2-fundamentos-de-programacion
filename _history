jamos la bandera visible un momento para que el jugador la vea\n        pause(1200)  # ajustar tiempo en ms si quieres que dure más/menos\n        # Terminar el juego declarando victoria\n        game.over(True)\n    else:\n        music.wawawawaa.play()\n        # solo mostramos color rojo breve como feedback\n        scene.set_background_color(2)\n        pause(500)\n        # resetear a color neutro\n        scene.set_background_color(9)\n        pause(300)\n        # continuar con la siguiente pregunta\n        mostrarpregunta()\n\n# ============================\n# Crear una nueva pregunta\n# ============================\ndef mostrarpregunta():\n    global indice, preguntaActual, expected, seleccion, opciones_text\n    # Elegir país al azar\n    indice = randint(0, len(paises) - 1)\n    preguntaActual = paises[indice]\n    expected = capitales[indice]\n    seleccion = 0\n    # Reseteamos el fondo a un color neutro (NO mostrar bandera aquí)\n    scene.set_background_color(9)\n    # Crear lista con las opciones (SOLO STRINGS)\n    opciones_text = [expected]\n    # Agregar incorrectas únicas\n    while len(opciones_text) < 4:\n        idx = randint(0, len(capitales) - 1)\n        if capitales[idx] not in opciones_text:\n            opciones_text.append(capitales[idx])\n\n    # Mezclar las opciones manualmente (Fisher–Yates)\n    n = len(opciones_text)\n    for i in range(n - 1, 0, -1):\n        j = randint(0, i)  # 0 <= j <= i\n        tmp = opciones_text[i]\n        opciones_text[i] = opciones_text[j]\n        opciones_text[j] = tmp\n\n    # Mostrar pregunta y opciones en recuadros separados\n    mostrarOpciones()\n\n# ============================\n# Variables globales iniciales\n# ============================\nindice = 0\npuntos = 0\nexpected = \"\"\npreguntaActual = \"\"\ntexto = \"\"\nseleccion = 0\nopciones_text: List[str] = []\nbanderas: List[Image] = []\ncapitales: List[str] = []\npaises: List[str] = []\n\npaises = [\"Colombia\", \"Francia\", \"Alemania\", \"Japon\"]\ncapitales = [\"Bogota\", \"Paris\", \"Berlin\", \"Tokio\"]\nbanderas = [\n    assets.image(\"\"\"\n        Col\n        \"\"\"),\n    assets.image(\"\"\"\n        FRA\n        \"\"\"),\n    assets.image(\"\"\"\n        GRM\n        \"\"\"),\n    assets.image(\"\"\"\n        JAP\n        \"\"\")\n]\n\n# Fondo inicial neutro\nscene.set_background_color(9)\n\n# Iniciar primera pregunta\nmostrarpregunta()","tilemap.g.jres":"{\n    \"transparency16\": {\n        \"data\": \"hwQQABAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA==\",\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"tilemapTile\": true\n    },\n    \"*\": {\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"dataEncoding\": \"base64\",\n        \"namespace\": \"myTiles\"\n    }\n}","tilemap.g.ts":"","images.g.jres":"{\n    \"image1\": {\n        \"data\": \"hwQQABAAAABVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIg==\",\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"displayName\": \"Colombia\"\n    },\n    \"image2\": {\n        \"data\": \"hwQQABAAAABVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIlVVhYiIKCIiVVWFiIgoIiJVVYWIiCgiIg==\",\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"displayName\": \"Col\"\n    },\n    \"image4\": {\n        \"data\": \"hwQQABAAAAD///8iIlJVVf///yIiUlVV////IiJSVVX///8iIlJVVf///yIiUlVV////IiJSVVX///8iIlJVVf///yIiUlVV////IiJSVVX///8iIlJVVf///yIiUlVV////IiJSVVX///8iIlJVVf///yIiUlVV////IiJSVVX///8iIlJVVQ==\",\n        \"mimeType\": \"image/x-mkcd-f4\",\n        \"displayName\": \"GRM\"\n    },\n    \"image3\": {\n        \"data\": \"hwQQABAAAAARERERERERERERERERERERERERERERERERERERERERERERERERERERERERIRERERER